<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controle.ControllerCadastroConsulta">
    <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <Menu fx:id="menuHome" mnemonicParsing="false" onAction="#irHome" text="Home"/>
        <Menu fx:id="botaoLogout" mnemonicParsing="false" onAction="#logout" text="Logout"/>
    </MenuBar>
    <Label layoutX="14.0" layoutY="27.0" prefHeight="35.0" prefWidth="168.0" text="Cadastro Consulta">
        <font>
            <Font size="19.0"/>
        </font>
    </Label>
    <TextField fx:id="campoMotivo" layoutX="57.0" layoutY="124.0" prefHeight="25.0" prefWidth="487.0"/>
    <TextField fx:id="campoAnimal" layoutX="57.0" layoutY="176.0" prefHeight="25.0" prefWidth="228.0"/>
    <TextField fx:id="campoVeterinario" layoutX="316.0" layoutY="175.0" prefHeight="25.0" prefWidth="228.0"/>
    <Button fx:id="botaoCadastrar" layoutX="221.0" layoutY="282.0" mnemonicParsing="false" onAction="#cadastrar"
            prefHeight="43.0" prefWidth="128.0" text="Cadastrar">
        <font>
            <Font size="16.0"/>
        </font>
    </Button>
    <Label layoutX="57.0" layoutY="101.0" prefHeight="21.0" prefWidth="60.0" text="Motivo:">
        <font>
            <Font size="15.0"/>
        </font>
    </Label>
    <Label layoutX="57.0" layoutY="155.0" prefHeight="21.0" prefWidth="98.0" text="ID do Animal:">
        <font>
            <Font size="15.0"/>
        </font>
    </Label>
    <Label layoutX="316.0" layoutY="155.0" prefHeight="21.0" prefWidth="147.0" text="CRMV do Veterinario:">
        <font>
            <Font size="15.0"/>
        </font>
    </Label>
    <DatePicker fx:id="campoData" layoutX="284.0" layoutY="228.0" prefHeight="25.0" prefWidth="147.0"/>
    <Label layoutX="139.0" layoutY="230.0" prefHeight="21.0" prefWidth="128.0" text="Data da Consulta:">
        <font>
          <Font size="15.0"/>
        </font>
    </Label>
</AnchorPane>
